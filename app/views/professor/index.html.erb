          <div class="mui-textfield mui-textfield--float-label">
            <input type="number">
            <label>CCN</label>
          </div>
          <div class="mui-textfield mui-textfield--float-label">
            <input type="text">
            <label>Course Name</label>
          </div>
          <div class="mui-textfield mui-textfield--float-label">
